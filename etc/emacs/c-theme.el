 '(highlight ((t (:background "#4f4f4f" :box (:line-width 1 :color "#5f5f5f")))))
 '(region ((t (:extend t :background "#1f1f1f" ))))
 '(shadow ((t (:foreground "#7f7f7f"))))
 '(secondary-selection ((t (:extend t :background "#7f007f"))))
 '(trailing-whitespace ((t (:background "#7f5f5f"))))
 '(font-lock-builtin-face ((t (:foreground "#9f9fff" :weight bold))))
