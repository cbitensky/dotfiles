 '(font-lock-warning-face ((t (:inherit error))))
 '(button ((t (:inherit link))))
 '(link ((t (:foreground "#9f9fff" :underline t))))
 '(link-visited ((t (:foreground "#df7fff" :underline t))))
 '(fringe ((t (:background "#5f5f5f" :foreground "#9f9f9f" :box (:line-width 2 :color "#5f5f5f")))))
 '(header-line ((t (:background "#666"))))
